// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model events {
  id Int @id @default(autoincrement())
  title String
  notifievent String
  clubname String  @unique
  img String
}

model authorcraft {
  id Int @id @default(autoincrement())
  title String
  timendata String
  venue     String
  eventdetail String

}

